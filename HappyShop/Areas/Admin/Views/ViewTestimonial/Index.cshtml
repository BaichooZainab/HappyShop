@model IEnumerable<Testimonial>

@{
    ViewData["Title"] = "Index";
}



<div class="card shadow border-0 my-4">
    <div class="card-header bg-dark bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h2 class="text-white py-2">View All Testimonials</h2>
            </div>
        </div>
    </div>
    <div class="card-body p-4">
        <div class="row pb-3">
            <div class="col-6">
            </div>
           
        </div>
        <table class="table table-bordered table-striped" id="tblData" style="width:100%">
            <thead>

                <tr>
                   
                    <th>Name</th>
                    <th>Testimonials</th>
                    <th>Image</th>
                    <th>Date</th>

                   


                    <th></th>

                </tr>
            </thead>



        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/testimonial.js"></script>
}
